				float temp_output_19_0 = ( step( tex2DNode2.r , temp_output_103_0 ) * temp_output_9_0 );
				
				float4 tex2DNode32 = SAMPLE_TEXTURE2D( _MainTex, sampler_MainTex, IN.ase_texcoord3.xy );
