				float3 ase_worldPos = mul(GetObjectToWorldMatrix(), v.vertex).xyz;
				float simplePerlin3D49 = snoise( ( ( _AxisScale * (( _LocalWorld )?( ase_worldPos ):( v.vertex.xyz )) ) + ( _PanningSpeed * _TimeParameters.x ) ) );
				float temp_output_38_0 = (0.0 + (simplePerlin3D49 - -1.0) * (1.0 - 0.0) / (1.0 - -1.0));
				float3 appendResult33 = (float3(( _Intensity * _AxisIntensity.x * temp_output_38_0 ) , ( _Intensity * _AxisIntensity.y * temp_output_38_0 ) , ( _Intensity * _AxisIntensity.z * temp_output_38_0 )));
				
				o.ase_texcoord3.xy = v.ase_texcoord.xy;
				o.ase_color = v.ase_color;
				
				//setting value to unused interpolator channels and avoid initialization warnings
